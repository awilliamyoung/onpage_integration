function cancel(){
	var gr1 = new GlideRecord('x_onpr_onpage_notification');
		gr1.addQuery('status',"failed");
		gr1.query();
	if(!gr1.hasNext()){
		return;
	}
	gr1.setValue('status',"cancelled");
	gr1.setValue('error',"Failed notification was cancelled by an administrator.");
	gr1.update();
}
